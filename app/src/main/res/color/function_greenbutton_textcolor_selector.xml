<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--按钮颜色的状态切换，默认为白色-->
    <item android:color="#80ffffff" android:state_pressed="true" />
    <item android:color="#80ffffff" android:state_focused="true" />
    <item android:color="#80ffffff" android:state_selected="true" />
    <item android:color="#ffffffff" />
</selector>